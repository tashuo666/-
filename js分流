function main(params) {
  // åˆ›å»ºä»£ç†ç»„çš„å‡½æ•°
  function createProxyGroup(name, type, icon, proxies) {
    // è®¾ç½®æ¯ä¸ªä»£ç†ç»„çš„å…¬å…±é…ç½®
    const commonConfig = {
      url: "http://www.gstatic.com/generate_204", // ç”¨äºæ£€æµ‹ä»£ç†æ˜¯å¦å¯ç”¨çš„ URL
      icon, // ä»£ç†ç»„çš„å›¾æ ‡
      lazy: true, // å»¶è¿ŸåŠ è½½
    };

    // æ ¹æ®ä»£ç†ç»„ç±»å‹è®¾ç½®ç‰¹å®šé…ç½®
    const typeConfig = type === "url-test" ? {
      interval: 300, // URL æµ‹è¯•çš„é—´éš”æ—¶é—´ï¼ˆç§’ï¼‰
      tolerance: 20, // å®¹å¿åº¦ï¼ˆmsï¼‰
      timeout: 2000 // è¶…æ—¶æ—¶é—´ï¼ˆmsï¼‰
    } : type === "load-balance" ? {
      strategy: "consistent-hashing" // è´Ÿè½½å‡è¡¡ç­–ç•¥
    } : {};

    // è¿”å›åˆ›å»ºçš„ä»£ç†ç»„é…ç½®
    return {
      name,
      type: type || "url-test", // é»˜è®¤ç±»å‹ä¸º url-test
      proxies: proxies.length > 0 ? proxies : ["DIRECT"], // å¦‚æœæ²¡æœ‰æŒ‡å®šä»£ç†ï¼Œé»˜è®¤ä½¿ç”¨ DIRECT
      ...commonConfig,
      ...typeConfig
    };
  }

  // é€šè¿‡æ­£åˆ™è¡¨è¾¾å¼è·å–ç¬¦åˆæ¡ä»¶çš„ä»£ç†
  function getProxiesByRegex(proxies, regex) {
    return proxies.filter(proxy => regex.test(proxy.name)).map(proxy => proxy.name);
  }

